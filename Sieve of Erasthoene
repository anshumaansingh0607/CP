#include<bits/stdc++.h>
using namespace std;
#define ll long long 
const int n=1e7;
vector<int>sieve(n+1,1);
void primesieve(vector<int>&sieve){
    sieve[0]=sieve[1]=0;
    for(ll i=2;i*i<=n;i++){
        if(sieve[i]==1){
            for(ll j=i*i;j<=n;j+=i){
                sieve[j]=0;
            }
        }
    }
}
int32_t main(){
   primesieve(sieve);
   cout<<sieve[997];
   
}
