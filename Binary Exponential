// Binary Exponential (A^B)
// Time complexity=O(logB)
#include<bits/stdc++.h>
using namespace std;
#define int long long int
int binaryExponential(int a, int b){
    int res=1;
    while(b>0){
        if(b&1)res*=a;
        a=a*a;
        b/=2;
    }
    return res;
}
int32_t main(){
    int a,b;
    cin>>a>>b;
    int ans=binaryExponential(a,b);
    cout<<ans<<endl;
}
